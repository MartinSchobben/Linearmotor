  shorttitle = {{{TONR}}},
  author = {Zhang, Zeyu and Li, Yu and Zhou, Weien and Chen, Xiaoqian and Yao, Wen and Zhao, Yong},
  year = {2021},
  month = dec,
  journal = {Computer Methods in Applied Mechanics and Engineering},
  volume = {386},
